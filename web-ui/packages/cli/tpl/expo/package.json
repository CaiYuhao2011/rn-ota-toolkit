{
  "name": "{slug}",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "node .env.development.js && expo start",
    "android": "node .env.development.js && rn-ota run --platform android",
    "andoird:release": "node .env.development.js && rn-ota run --platform android --variant release",
    "bundle:dev": "node .env.development.js && rn-ota build --platform android -t bundle --project .",
    "bundle:prod": "node .env.production.js && rn-ota build --platform android -t bundle --project .",
    "release:dev": "node .env.development.js && rn-ota build --platform android -t apk --project .",
    "release:prod": "node .env.production.js && rn-ota build --platform android -t apk --project .",
    "android:prebuild": "expo prebuild -p android --clean",
    "sync:dev:config": "node .env.development.js",
    "sync:prod:config": "node .env.production.js",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "help": "rn-ota help"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "~54.0.25",
    "expo-application": "~7.0.7",
    "expo-battery": "~10.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.18",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-intent-launcher": "^13.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-navigation-bar": "~5.0.9",
    "expo-router": "~6.0.15",
    "expo-screen-orientation": "~9.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.13",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "react-native-zip-archive": "^7.0.2",
    "rn-ota-client": "^1.8.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/eslint-config": "0.78.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.1.0",
    "ajv": "^8.12.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.13",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "rn-ota-cli": "^1.4.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-restart",
          "react-native-zip-archive"
        ]
      }
    }
  }
}
